{
  "cli": {
    "version": ">= 16.17.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_WEB_VIEW_BASE_URL": "https://m.boardbuddi.com"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
